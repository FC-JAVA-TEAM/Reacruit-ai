# Environment Variables Template
# Copy this file to .env and fill in your actual values
# This file can be committed to Git as it contains no sensitive data

# Database credentials
DB_USERNAME=your_database_username
DB_PASSWORD=your_database_password

# Fuelix AI credentials
FUELIX_API_BASE_URL=https://api-beta.fuelix.ai
FUELIX_API_TOKEN=your_fuelix_api_token_here
FUELIX_API_MODEL=claude-3-7-sonnet
FUELIX_API_EMBEDDING_MODEL=text-embedding-ada-002

# Spring profile to use (local, dev, prod)
SPRING_PROFILES_ACTIVE=local

# Production-specific environment variables (for Cloud Run deployment)
SPRING_DATASOURCE_URL=jdbc:postgresql:///postgres?cloudSqlInstance=your-project:region:instance&socketFactory=com.google.cloud.sql.postgres.SocketFactory
SPRING_JPA_PROPERTIES_HIBERNATE_DIALECT=org.hibernate.dialect.PostgreSQLDialect
SPRING_JPA_HIBERNATE_DDL_AUTO=update
VAADIN_PRODUCTION_MODE=true
